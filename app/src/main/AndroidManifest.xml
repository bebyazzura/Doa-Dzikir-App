<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.DoaDzikir"
        tools:targetApi="31">
        <activity
            android:name=".DzikirPetangActivity"
            android:exported="true" />
        <activity
            android:name=".DzikirPagiActivity"
            android:exported="true" />
        <activity
            android:name=".DetailArtikelActivity"
            android:exported="true" />
        <activity
            android:name=".PagiPetangDzikirActivity"
            android:exported="true" />
        <activity
            android:name=".HarianDzikirDoaActivity"
            android:exported="true" />
        <activity
            android:name=".SetiapSaatDzikirActivity"
            android:exported="true" />
        <activity
            android:name=".QauliyahShalatActivity"
            android:exported="true" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:screenOrientation="fullSensor"
            android:theme="@style/Theme.SplashScreenBase"
            tools:ignore="LockedOrientationActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>